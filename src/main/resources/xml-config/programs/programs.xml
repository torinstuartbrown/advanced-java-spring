<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="legomastersProgram" class="platform.codingnomads.co.corespring.lab.Program">
        <property name="name" value="Lego Masters"/>
        <property name="season" value="1"/>
        <property name="episode" value="2"/>
    </bean>

    <bean id="aloneProgram" class="platform.codingnomads.co.corespring.lab.Program">
        <property name="name" value="Alone"/>
        <property name="season" value="4"/>
        <property name="episode" value="7"/>
    </bean>
</beans>

